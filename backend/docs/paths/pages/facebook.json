{
  "/pages/facebook": {
    "get": {
      "summary": "Get all facebook pages",
      "tags": [
        "Pages"
      ],
      "security": [
        {
          "bearerAuth": []
        }
      ],
      "responses": {
        "200": {
          "description": "List of facebook pages",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/Page"
                }
              }
            }
          }
        }
      }
    }
  }
}